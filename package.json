    "type": "git",
    "url": "https://github.com/seolux/Astro-Gemini.git"
  },
  "bugs": {
    "url": "https://github.com/seolux/Astro-Gemini/issues"
  },
  "homepage": "https://astro.lu",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "serve": "^14.2.1"
  }
